# RDS Dashboard Infrastructure Configuration
# Copy this file to .env and update with your values

# AWS Account and Region
AWS_ACCOUNT_ID=123456789012
AWS_REGION=ap-southeast-1

# Environment (prod, dev, test)
ENVIRONMENT=prod

# Cross-Account Configuration
EXTERNAL_ID=rds-dashboard-unique-id-12345
CROSS_ACCOUNT_ROLE_NAME=RDSDashboardCrossAccountRole

# Target Accounts (JSON array)
TARGET_ACCOUNTS=["123456789012","234567890123","345678901234"]

# Target Regions (JSON array)
TARGET_REGIONS=["ap-southeast-1","eu-west-2","ap-south-1","us-east-1"]

# Monitoring
SNS_EMAIL=dba-team@company.com

# Cache Configuration
METRICS_TTL_SECONDS=300
CACHE_HIT_THRESHOLD=0.7

# Logging
LOG_LEVEL=INFO

# CloudWatch Namespace
CLOUDWATCH_NAMESPACE=RDSDashboard
